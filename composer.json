{
	"name": "shineunited/composer-build",
	"type": "composer-plugin",
	"license": "MIT",
	"require": {
		"php": ">=5.6",
		"composer-plugin-api": "^1.0 || ^2.0"
	},
	"require-dev": {
		"composer/composer": ">=1.0",
		"phpunit/phpunit": ">=5.5.0"
	},
	"autoload": {
		"psr-4": {
			"ShineUnited\\ComposerBuild\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"ShineUnited\\ComposerBuild\\Tests\\": "tests/"
		}
	},
	"extra": {
		"class": "ShineUnited\\ComposerBuild\\Plugin"
	}
}
